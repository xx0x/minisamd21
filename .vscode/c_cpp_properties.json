{
    "configurations": [
        {
            "name": "Mac OS (brew)",
            "includePath": [
                "${workspaceFolder}/inc/*",
                "${workspaceFolder}/platform/CMSIS/5.4.0/CMSIS/Core/Include/*",
                "${workspaceFolder}/platform/CMSIS-Atmel/1.2.2/CMSIS/Device/ATMEL/samd21/include/*"
            ],
            "defines": [
                "__SAMD21E18A__"
            ],
            "compilerPath": "/opt/homebrew/bin/arm-none-eabi-gcc",
            "cStandard": "c11",
            "cppStandard": "c++23",
            "intelliSenseMode": "gcc-arm",
            "browse": {
                "path": [
                    "${workspaceFolder}/src"
                ],
                "limitSymbolsToIncludedHeaders": true
            }
        }
    ],
    "version": 4
}